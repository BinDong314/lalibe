<?xml version="1.0"?>
<lalibe>
<annotation>
;
; Test input file for lalibe
;
</annotation>
<Param>
<InlineMeasurements>

<elem>
  <Name>HDF5_READ_NAMED_OBJECT</Name>
  <Frequency>1</Frequency>
  <NamedObject>
    <object_id>PS_prop</object_id>
    <object_type>LatticePropagator</object_type>
  </NamedObject>
  <File>
    <file_name>./test_propagator.h5</file_name>
    <path>/sh_sig2p0_n5</path>
    <obj_name>PS_prop</obj_name>
  </File>
</elem>

<elem>
  <Name>SINK_SMEAR</Name>
  <Frequency>1</Frequency>
  <Param>
    <version>5</version>
    <Sink>
      <version>2</version>
      <SinkType>SHELL_SINK</SinkType>
      <j_decay>3</j_decay>
      <SmearingParam>
        <wvf_kind>GAUGE_INV_GAUSSIAN</wvf_kind>
        <wvf_param>2.0</wvf_param>
        <wvfIntPar>5</wvfIntPar>
        <no_smear_dir>3</no_smear_dir>
      </SmearingParam>
    </Sink>
  </Param>
  <NamedObject>
    <gauge_id>default_gauge_field</gauge_id>
    <prop_id>PS_prop</prop_id>
    <smeared_prop_id>SS_prop</smeared_prop_id>
  </NamedObject>
</elem>

<elem>
  <Name>PIPI_SCATTERING</Name>
  <PipiParams>
    <p2_max>1</p2_max>
    <particle_list>
      <elem>piplus</elem>
    </particle_list>
    <h5_file_name>./lalibe_pipi_spectrum.h5</h5_file_name>
    <obj_path>/SS</obj_path>
  </PipiParams>
  <NamedObject>
    <quark_prop_1>SS_prop</quark_prop_1>
    <quark_prop_2>SS_prop</quark_prop_2>
    <quark_prop_3>SS_prop</quark_prop_3>
    <quark_prop_4>SS_prop</quark_prop_4>
  </NamedObject>
</elem>

</InlineMeasurements>
<nrow>4 4 4 8</nrow>
</Param>

<RNG>
  <Seed>
    <elem>11</elem>
    <elem>11</elem>
    <elem>11</elem>
    <elem>0</elem>
  </Seed>
</RNG>

<Cfg>
  <cfg_type>WEAK_FIELD</cfg_type>
  <cfg_file>dummy</cfg_file>
</Cfg>
</lalibe>

<?xml version="1.0"?>
<lalibe>
<annotation>
;
; Test input file for lalibe
;
</annotation>
<Param>
<InlineMeasurements>

<elem>
  <Name>MAKE_SOURCE</Name>
  <Frequency>1</Frequency>
  <Param>
    <version>6</version>
    <Source>
      <version>2</version>
      <SourceType>SHELL_SOURCE</SourceType>
      <j_decay>3</j_decay>
      <t_srce>0 0 0 3</t_srce>
      <SmearingParam>
        <wvf_kind>GAUGE_INV_GAUSSIAN</wvf_kind>
        <wvf_param>2.0</wvf_param>
        <wvfIntPar>5</wvfIntPar>
        <no_smear_dir>3</no_smear_dir>
      </SmearingParam>
    </Source>
  </Param>
  <NamedObject>
    <gauge_id>default_gauge_field</gauge_id>
    <source_id>sh_source_0</source_id>
  </NamedObject>
</elem>

<elem>
  <Name>HDF5_WRITE_NAMED_OBJECT</Name>
  <Frequency>1</Frequency>
  <NamedObject>
    <object_id>sh_source_0</object_id>
    <object_type>LatticePropagator</object_type>
  </NamedObject>
  <File>
    <file_name>./test_propagator.h5</file_name>
    <path>/sh_sig2p0_n5</path>
    <obj_name>sh_sig2p0_n5</obj_name>
  </File>
</elem>

<elem>
  <Name>PROPAGATOR</Name>
  <Frequency>1</Frequency>
  <Param>
    <version>10</version>
    <quarkSpinType>FULL</quarkSpinType>
    <obsvP>false</obsvP>
    <numRetries>1</numRetries>
    <FermionAction>
      <FermAct>UNPRECONDITIONED_CLOVER</FermAct>
      <Mass>0.5</Mass>
      <clovCoeff>1.17</clovCoeff>
      <FermionBC>
        <FermBC>SIMPLE_FERMBC</FermBC>
        <boundary>1 1 1 -1</boundary>
      </FermionBC>
    </FermionAction>
    <InvertParam>
      <invType>CG_INVERTER</invType>
      <RsdCG>1.0e-15</RsdCG>
      <MaxCG>1000</MaxCG>
    </InvertParam>
  </Param>
  <NamedObject>
    <gauge_id>default_gauge_field</gauge_id>
    <source_id>sh_source_0</source_id>
    <prop_id>PS_prop</prop_id>
  </NamedObject>
</elem>

<elem>
  <Name>HDF5_WRITE_ERASE_NAMED_OBJECT</Name>
  <Frequency>1</Frequency>
  <NamedObject>
    <object_id>PS_prop</object_id>
    <object_type>LatticePropagator</object_type>
  </NamedObject>
  <File>
    <file_name>./test_propagator.h5</file_name>
    <path>/sh_sig2p0_n5</path>
    <obj_name>PS_prop</obj_name>
  </File>
</elem>

</InlineMeasurements>
<nrow>4 4 4 8</nrow>
</Param>

<RNG>
  <Seed>
    <elem>11</elem>
    <elem>11</elem>
    <elem>11</elem>
    <elem>0</elem>
  </Seed>
</RNG>

<Cfg>
  <cfg_type>WEAK_FIELD</cfg_type>
  <cfg_file>dummy</cfg_file>
</Cfg>
</lalibe>
